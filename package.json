{
  "name": "kythera",
  "version": "1.0.0",
  "description": "A compact language with first-class types that compiles to JS",
  "main": "src/kythera.ts",
  "scripts": {
    "start": "ts-node src/kythera.ts",
    "build-rt": "tsc src/runtime/* --outdir lib/",
    "watch-rt": "tsc --watch src/runtime/* --outdir lib/",
    "prettier": "prettier --write 'src/*'",
    "test": "ts-node test/test.ts",
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/dejawu/kythera.git"
  },
  "author": "Holly",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/dejawu/kythera/issues"
  },
  "homepage": "https://gitlab.com/dejawu/kythera#readme",
  "dependencies": {
    "prettier": "^2.4.1",
    "terser": "^5.9.0",
    "ts-node": "^10.3.1",
    "ts-pattern": "^3.3.3",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/baretest": "^2.0.0",
    "@types/node": "^16.11.2",
    "@types/prettier": "^2.4.1",
    "baretest": "^2.0.0"
  }
}
